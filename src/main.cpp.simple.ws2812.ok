#include <Arduino.h>
#include "WS2812Led.h"

static const uint8_t WS2812LedPin = 18;
WS2812Led wsLED(WS2812LedPin, 25);

void setup() {
  Serial.begin(115200);
  delay(100); // allow to flush boot messages
  wsLED.Brightness(5);
  delay(1000);
  wsLED.UpdateAll(wsLED.OLIVE);
  delay(1000);
  wsLED.UpdateAll(wsLED.RED);
  delay(1000);
  wsLED.UpdateAll(wsLED.YELLOW);
  delay(1000);
}

void loop() {

}
