#include <remote.h>

void startTest()
{
  while (true)
  {
    Serial.println("off");
    pinMode(16, OUTPUT);
    digitalWrite(16, LOW);
    delay(500);
    Serial.println("on");
    pinMode(16, OUTPUT);
    digitalWrite(16, HIGH);
    delay(500);
  }
}

